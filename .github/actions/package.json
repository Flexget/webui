{
  "name": "actions",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release:bump": "NODE_ENV=release babel-node ./scripts/bump-version.js",
    "release:upload": "NODE_ENV=release babel-node ./scripts/create-release.js",
    "package": "yarn package:deploy && yarn package:lint && yarn package:status",
    "package:deploy": "ncc build deployment/index.ts -o deployment/lib",
    "package:lint": "ncc build eslint/index.ts -o eslint/lib",
    "package:status": "ncc build deployment-status/index.ts -o deployment-status/lib"
  },
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^2.0.0",
    "@babel/code-frame": "^7.5.5",
    "lodash.chunk": "^4.2.0"
  },
  "devDependencies": {
    "@babel/node": "^7.7.4",
    "@types/babel__code-frame": "^7.0.1",
    "@types/lodash.chunk": "^4.2.6",
    "@zeit/ncc": "^0.20.5"
  }
}
